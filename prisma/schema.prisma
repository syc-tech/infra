
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int      @id @default(autoincrement())
  user_name      String   @unique
  password       String
  openapi_token  String?  @default("")
  user_profile   Json     @default("{}")
  data           Json     @default("{}")
}
